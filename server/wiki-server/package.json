{
  "name": "wiki-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/app.js",
    "server:prod": "node ./dist/app.js",
    "dev": "npm run server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "set NODE_ENV=production npm-run-all clean build server",
    "watch": "nodemon"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^3.4.0",
    "mongoose": "^5.8.0",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "nodemon": "^2.0.1"
  }
}
